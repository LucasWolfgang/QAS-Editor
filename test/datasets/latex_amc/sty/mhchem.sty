\int_compare:nT { \l__mhchem_option_version_int < 4 }
  {
    \newcommand*\cmath[1]{\__mhchem_output_escapeFromMathToItalicMath:n{#1}}
    \DeclareRobustCommand\cf[2][]{\mhchem@cf[#1]{#2}}
    \DeclareRobustCommand\cee[1]{\mhchem@cee{#1}}
  }

